; demoV.a demos the usage of mul, div, and rem instructions
        mov r0, 10
        mov r1, 3
        mul r0, r1
        dout r0 ; should output 30
        nl

        mov r0, 10
        mov r1, 2
        div r0, r1
        dout r0 ; should output 5
        nl

        mov r0, 10
        mov r1, 3
        rem r0, r1
        dout r0 ; should output 1
        halt